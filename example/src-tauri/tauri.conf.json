{
  "$schema": "https://schema.tauri.app/config/2",
  "app": {
    "security": {
      "csp": null
    },
    "windows": [
      {
        "title": "Example Desktop",
        "width": 800,
        "fullscreen": false,
        "height": 600,
        "resizable": true
      }
    ]
  },
  "build": {
    "devUrl": "http://localhost:4000",
    "frontendDist": "http://localhost:4000"
  },
  "bundle": {
    "active": true,
    "externalBin": [
      "../burrito_out/desktop"
    ],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "macOS": {
      "dmg": {
        "size": 2500000
      }
    },
    "targets": "all"
  },
  "identifier": "you.app.example-desktop",
  "productName": "Example Desktop",
  "version": "0.1.0"
}