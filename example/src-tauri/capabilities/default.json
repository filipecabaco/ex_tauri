{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main application window",
  "windows": ["main"],
  "permissions": [
    "shell:allow-execute",
    "shell:allow-spawn",
    {
      "identifier": "shell:allow-execute",
      "allow": [
        {
          "name": "desktop",
          "sidecar": true
        }
      ]
    }
  ]
}
